<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GBBDMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GBBDMobile"
    Shell.FlyoutBehavior="Disabled"
    Title="GBBDMobile">

    <TabBar>
        <Tab Title="Список">
            <ShellContent
                Title="Список"
                ContentTemplate="{DataTemplate local:Pages.PageMain}"
                Route="MainPage" />
        </Tab>
        <Tab Title="Новое дтп">
            <ShellContent
                Title="Новое дтп"
                ContentTemplate="{DataTemplate local:Pages.PageNewDTP}"
                Route="MainPage" />
        </Tab>
    </TabBar>

</Shell>
